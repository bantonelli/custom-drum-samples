<div id="st-container" class="st-container st-effect-4">
    <!--
        example menus
        these menus will be on top of the push wrapper
    -->
    <!-- content push wrapper -->
    <div class="st-pusher">
        <!--
            example menus
            these menus will be under the push wrapper
        -->

        <nav class="st-menu st-effect-7" id="menu-7">
            <ul>
                <li>{{#link-to 'index'}}Home{{/link-to}}</li>
                <li>{{#link-to 'kb-learn-more'}}How It Works{{/link-to}}</li>
                <li>{{#link-to 'about'}}About Us{{/link-to}}</li>
                <li>{{#link-to 'legal'}}Legal{{/link-to}}</li>
                <li>{{#link-to 'team-members'}}Team Members{{/link-to}}</li>
                <li>{{#link-to 'contact'}}Contact{{/link-to}}</li>

                {{!-- <li class="multilink">
                    <a><label for="menu-toggle">Community</label></a>
                    <input type="checkbox" id="menu-toggle"/>
                    <ul id="menu">
                      <li><a href="#">Blog</a></li>
                      <li><a href="#">Forum</a></li>
                      <li><a href="#">Mail Chimp</a></li>
                    </ul>
                </li> --}}
            </ul>
        </nav>
    <!-- Nav Button to Trigger sidebar
                            <div id="st-trigger-effects" class="col-xs-2">
                                <button class="nav-button icon-menu" data-effect="st-effect-7"></button>
                            </div>
    -->
        <div class="st-content z-index-fix"><!-- this is the wrapper for the content -->
            <div class="st-content-inner"><!-- extra div for emulating position:fixed of the menu -->
                <!-- Top Navigation -->                                <!-- 5 -->
                <!-- 0 -->
                {{liquid-modal}}
                <div class="loader" title="0">
                    <div class="svg-wrapper"> 
                        <svg version="1.1" id="loader-1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"
                        width="120px" height="120px" viewBox="0 0 40 40" enable-background="new 0 0 40 40" xml:space="preserve">
                        <path opacity="0.2" fill="#000" d="M20.201,5.169c-8.254,0-14.946,6.692-14.946,14.946c0,8.255,6.692,14.946,14.946,14.946
                        s14.946-6.691,14.946-14.946C35.146,11.861,28.455,5.169,20.201,5.169z M20.201,31.749c-6.425,0-11.634-5.208-11.634-11.634
                        c0-6.425,5.209-11.634,11.634-11.634c6.425,0,11.633,5.209,11.633,11.634C31.834,26.541,26.626,31.749,20.201,31.749z"/>
                        <path fill="#000" d="M26.013,10.047l1.654-2.866c-2.198-1.272-4.743-2.012-7.466-2.012h0v3.312h0
                            C22.32,8.481,24.301,9.057,26.013,10.047z">
                            <animateTransform attributeType="xml"
                              attributeName="transform"
                              type="rotate"
                              from="0 20 20"
                              to="360 20 20"
                              dur="0.5s"
                              repeatCount="indefinite"/>
                        </path>
                        </svg>
                    </div>
                
                </div>
                <header role="banner" class="cds-navbar">
                    <div class="container cds-nav-container">
                        <div class="row">
                            <div id="st-trigger-effects" class="col-xs-1">
                                <div id="main-nav-image" data-effect="st-effect-7">
                                    
                                </div>
                            </div>
                            <div class="col-xs-2">
                                <div class="row">
                                    <div id="nav_logo">
                                        {{#link-to 'index'}}
                                            <img src="/images/nav_logo.png" title="BeatParadigm" alt="BeatParadigm">
                                        {{/link-to}}
                                    </div>
                                </div>
                            </div>
                            <nav class="main-nav col-xs-3 col-xs-offset-3">
                                <ul class="row">
                                    <!-- insert more links here -->
                                    <li>{{#link-to 'kitbuilder' class="col-xs-6 col-xs-offset-6"}}Kit Builder{{/link-to}}</li>
                                    {{!-- <li>{{#link-to 'browse-templates' class="col-xs-6"}}Browse Templates{{/link-to}}</li> --}}
                                </ul>
                            </nav>
                            <nav id="main-nav" class="main-nav col-xs-3">
                                <ul class="row">
                                    <!-- insert more links here -->
                                    {{#if session.isAuthenticated}}
                                        <li class="col-xs-8 username-display">
                                            <label class="col-xs-12 cd-signin" style="width: 100%; cursor: pointer; text-align:center; overflow: visible; white-space: nowrap;" for="usermenu-toggle">
                                            {{session.currentUser.username}}
                                            </label>
                                            <input type="checkbox" id="usermenu-toggle"/>
                                            <ul class="user-dropdown">
                                                <li>{{#link-to 'account-settings' class="col-xs-12"}}Account Settings{{/link-to}}</li>
                                                <li>{{#link-to 'user-profile' class="col-xs-12"}}User Profile{{/link-to}}</li>
                                                <li><a class="col-xs-12" {{action 'invalidateSession'}}>Sign Out</a></li>
                                            </ul>
                                        </li>
                                    {{else}}
                                        <li>{{#link-to (query-params login=true) classNames="col-xs-6 cd-signin" }}Sign In{{/link-to}}</li>
                                        <li>{{#link-to 'register' classNames="col-xs-6 cd-signin" }}Register{{/link-to}}</li>
                                    {{/if}}
                                </ul>
                            </nav>
                        </div>
                    </div>
                    <div class="appnav">
                          {{outlet 'appnav'}}
                    </div>
                </header>
                {{liquid-outlet}}
            </div><!-- /st-content-inner -->
        </div><!-- /st-content -->
    </div><!-- /st-pusher -->
</div><!-- /st-container -->


