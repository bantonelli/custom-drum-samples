{{#each kit in kits}}
    <div data-datecreated="{{unbound kit.date_created}}" class="col-xs-4 sound-kit mix {{unbound kit.displayedTags}}"  {{action 'setCurrent' kit}}>
        <img class="" src="{{unbound kit.image}}" alt="{{unbound kit.name}}">
        <div class="cds-image-div-wrapper"><div class="cds-image-div"></div></div>
    </div>
{{/each}}
