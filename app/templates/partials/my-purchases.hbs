<div class="row">
    <section class="col-xs-10 col-xs-offset-1 form-section">
        <div class="row">
            <h4 class="col-xs-12 icon-side_drum">My Kitbuilder Purchases</h4>
            <ul class="col-xs-12 custom-kit-list">
            	{{#each purchase in profile.kitbuilder_purchases}}
                	<a href="{{unbound purchase.zip_file}}" class="icon-download">{{purchase.name}}</a>                	
            	{{/each}}
            </ul>
        </div>
    </section>
</div>